{
  "name": "nostalgik-mc",
  "version": "0.12.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky install",
    "lint": "ng lint",
    "build:release": "yarn build --configuration=production --aot --baseHref=/nostalgik-mc/",
    "build:release:alter": "yarn build --configuration=production --aot --baseHref=/NostalgikVanilla/"
